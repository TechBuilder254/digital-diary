{
  "name": "digital-diary",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "node kill-ports.js || true",
    "start": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev": "vercel dev",
    "dev:api": "node server-dev.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "test:register-login": "node scripts/test-register-login.js",
    "test:deployment": "node scripts/test-deployment.js",
    "test:login": "node scripts/test-login.js",
    "check:users": "node scripts/check-users.js",
    "clean": "node kill-ports.js",
    "docker": "node start.js",
    "docker:build": "node start.js --build",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "vercel": "^48.8.2"
  }
}
